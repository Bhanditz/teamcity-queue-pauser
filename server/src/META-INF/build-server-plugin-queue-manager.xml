<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
        default-autowire="constructor"
        >
  <!-- declare all beans you like Spring Dependency Injection to create -->
  <!-- see http://static.springsource.org/spring/docs/3.0.x/spring-framework-reference/html/beans.html -->

  <!-- sample: 
  <bean class="bean class fully qualified name"/>
  -->

  <bean class="jetbrains.buildServer.queueManager.settings.SettingsManagerImpl"/>

  <bean class="jetbrains.buildServer.queueManager.StartStopWaitPrecondition"/>

  <bean id="buildQueuePauseButton" class="jetbrains.buildServer.queueManager.pages.BuildQueuePauseButton"
        init-method="register">
    <property name="placeId" value="ALL_PAGES_FOOTER"/>
    <property name="pluginName" value="samplePlugin"/>
  </bean>

  <bean class="jetbrains.buildServer.queueManager.pages.QueueStateController"/>


</beans>
